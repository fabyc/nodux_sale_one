<tryton>
    <data>
        <!-- relacionado-->
        <record model="ir.action.act_window" id="act_party_receivable_form">
            <field name="name">Por Cobrar</field>
            <field name="res_model">sale.sale</field>
            <field name="domain" eval="[('party', 'in', Eval('active_ids')), ('state', '=', 'confirmed'), ('residual_amount', '>=', 0)]" pyson="1"/>
        </record>
        <record model="ir.action.keyword"
                id="act_open_party_receivable_keyword1">
            <field name="keyword">form_relate</field>
            <field name="model">party.party,-1</field>
            <field name="action" ref="act_party_receivable_form"/>
        </record>

    </data>
</tryton>
